<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="AproTools.App"
             xmlns:local="using:AproTools"
             RequestedThemeVariant="Dark">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>
  
    <Application.Styles>
        <FluentTheme />
		<StyleInclude Source="avares://AproTools/Assets/Icons.axaml"></StyleInclude>
    </Application.Styles>

	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<MergeResourceInclude Source="/Assets/Styles.axaml" />
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
		<FontFamily x:Key="DigitalFont">avares://AproTools/Assets/Fonts/digital-7.ttf#digital-7</FontFamily>
	</Application.Resources>

</Application>