<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<ResourceDictionary.ThemeDictionaries>
		
		<ResourceDictionary x:Key='Light'>
			<SolidColorBrush x:Key='bgBox'>#ffffff</SolidColorBrush>
			<SolidColorBrush x:Key='bgLeftMenu'>#f2f2f2</SolidColorBrush>
		</ResourceDictionary>
		<ResourceDictionary x:Key='Dark'>
			<SolidColorBrush x:Key='bgBox'>#2d2d2d</SolidColorBrush>
			<SolidColorBrush x:Key='bgLeftMenu'>#171717</SolidColorBrush>
		</ResourceDictionary>
		
	</ResourceDictionary.ThemeDictionaries>

</ResourceDictionary>
